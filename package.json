    "typings": "~1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4",
    "zone.js": "~0.6.12"
  },
  "dependencies": {
    "equals": "^1.0.5",
    "ng2-material-dropdown": "0.1.2"
  },
  "keywords": [
    "angular 2",
    "angular 2 select",
    "angular 2 select component",
    "angular 2 select dropdown"
  ],
  "repository": {
