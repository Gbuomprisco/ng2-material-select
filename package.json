    "ts-node": "^3.0.2",
    "tslint": "^4.5.1",
    "typescript": "2.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "zone.js": "~0.8.5"
  },
  "dependencies": {
    "equals": "^1.0.5",
    "ng2-material-dropdown": "0.7.5"
  },
  "keywords": [
    "angular 2",
